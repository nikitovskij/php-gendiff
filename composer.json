{
    "name": "nikitovskij/php-gendiff",
    "description": "CLI utility for fainding discrepancies in configuration files.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nikitovskij",
            "email": "nbaranovsky24@gmail.com"
        }
    ],
    "require": {
        "docopt/docopt": "*",
        "funct/funct": "^1.0@dev"
    },
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Php\\Package\\": "src",
            "Php\\Package\\Tests\\": "tests"
        },
        "files": [
            "src/GenDiff.php"
        ]
    },
    "scripts": {
        "phpcs": "phpcs"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0@dev"
    }
